{
    "version": 2,
    "name": "microsblog",
    "builds": [
        {
            "src": "compile/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["compile/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "compile/index.js"
        }
    ],
    "cleanUrls": true
}