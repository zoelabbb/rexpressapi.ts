{
    "version": 2,
    "name": "microsblog",
    "builds": [
        {
            "src": "compile/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "compile/index.js"
        }
    ],
    "cleanUrls": true,
    "functions": {
        "compile/index.js": {
            "memory": 3008,
            "maxDuration": 30
        },
        "compile/*.js": {
            "memory": 3008,
            "maxDuration": 30
        },
        "compile/controller/*.js": {
            "memory": 3008,
            "maxDuration": 30
        },
        "compile/routes/*.js": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}